# Database Configuration
DATABASE_URL="postgresql://clinic_user:clinic_pass@localhost:5432/clinic_db?schema=clinic"

# Database Credentials (for docker-compose)
POSTGRES_USER=clinic_user
POSTGRES_PASSWORD=clinic_pass
POSTGRES_DB=clinic_db

# Scraping Configuration
SCRAPING_DELAY_MS=1500
SCRAPING_MAX_RETRIES=3
SCRAPING_TIMEOUT_MS=30000
SCRAPING_CONCURRENCY=3
SCRAPING_CITIES="Lima,Bogotá,Madrid"
SCRAPING_SPECIALTIES="Cardiólogo,Dermatólogo,Pediatra"
MAX_SERVICES_COUNT=5
MAX_AVAILABILITY_SLOTS_COUNT=5
MAX_DOCTORS_PER_SEARCH=3

# Data Generation Configuration
PATIENTS_COUNT=200
APPOINTMENTS_COUNT=1000

# Logging Configuration
LOG_LEVEL=info
NODE_ENV=development
